{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
	"target": "es2016",
    "sourceMap": true,
	"downlevelIteration": true,
    "types" : ["node"],
	"lib": ["esnext", "dom.iterable", "dom"]
  },
  "include": [
    "index.ts"
  ],
  "exclude": [
    "node_modules",
    "**/*.spec.ts"
  ]
}
